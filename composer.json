{
    "name": "gurucomkz/silverstripe-miniadminbar",
    "description": "Mini adminbar for quick access to the page editor",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Gurucomkz\\MiniAdminBar\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
